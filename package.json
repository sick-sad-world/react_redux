    ],
    "env": {
      "production": {
        "presets": [
          "react-optimize"
        ]
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "browser-jsonp": "^1.1.5",
    "classnames": "^2.2.5",
    "lodash-es": "^4.17.2",
    "moment": "^2.17.1",
    "prop-types": "^15.5.8",
    "react": "^15.3.2",
    "react-datetime": "^2.7.5",
    "react-dom": "^15.3.2",
    "react-lazy-load": "^3.0.10",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.2",
    "react-sortable-hoc": "^0.6.3",
    "react-transition-group": "^1.2.0",
    "react-virtualized": "^9.7.5",
    "recharts": "^1.0.0-alpha.0",
