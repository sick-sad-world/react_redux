    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "2.2.0",
