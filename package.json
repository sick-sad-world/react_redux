{
  "name": "trendolizer_ui",
  "version": "0.0.1",
  "description": "",
  "main": "assets/app.js",
  "scripts": {
    "test:output": "jest --json --outputFile=jest-test-results.json",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "webpack-dev-server --hot --inline --port=3000 --public=http://localhost:3000/",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "prebundle": "rimraf ./dist",
    "bundle": "webpack --progress",
    "prebuild": "rimraf ./build",
    "build": "webpack -p --progress",
    "clean": "rimraf ./dist ./build ./coverage jest-test-results.json",
    "precommit": "npm test"
  },
  "author": "Ihor Koptielov",
  "license": "MIT",
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "plugins": [
      "lodash"
    ],
    "env": {
      "production": {
        "presets": [
          "react-optimize"
        ]
      },
      "test": {
        "presets": [
          "env",
          "react",
          "stage-0"
        ]
      }
    }
  },
  "jest": {
    "setupFiles": ["jest/setup.js"],
    "watchPathIgnorePatterns" : ["/node_modules/", "jest-test-results.json", "**/*.json"],
    "verbose": true
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.5",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@storybook/react": "^3.3.14",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^4.0.0",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^3.1.3",
    "react-test-utils": "0.0.1",
    "sass-loader": "^6.0.6",
    "storybook-addon-jest": "0.0.1",
    "style-loader": "^0.20.1",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1",
    "yargs": "^11.0.0"
  }
}
