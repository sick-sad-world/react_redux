{
  "name": "trendolizer_webpack",
  "version": "0.0.1",
  "description": "Trendolizer build system based on webpack",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourapp.git"
  },
  "author": "Igor Koptelov",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./demo ./build",
    "test": "jest --watch",
    "start": "node webpack.server.js",
    "prebundle": "rimraf ./demo",
    "bundle": "cross-env NODE_ENV=development BUNDLE=true PLATFORM=web webpack",
    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production PLATFORM=web webpack"
  },
  "babel": {
    "presets": [
      "react",
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "stage-2"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "jest": {},
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "browser-jsonp": "^1.1.5",
    "classnames": "^2.2.5",
    "lodash": "^4.17.2",
    "lodash-es": "^4.17.2",
    "moment": "^2.17.1",
    "react": "^15.3.2",
    "react-datetime": "^2.7.5",
    "react-dom": "^15.3.2",
    "react-lazy-load": "^3.0.10",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.2",
    "react-sortable-hoc": "^0.6.3",
    "react-virtualized": "^9.7.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.0",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.3.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.5.8",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "next",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "sinon": "^2.1.0",
    "style-loader": "^0.16.1",
    "svg-inline-loader": "^0.7.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  }
}
