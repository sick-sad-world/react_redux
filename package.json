{
  "name": "trendolizer",
  "description": "React-Redux trendolizer client",
  "author": "Igor Koptelov",
  "version": "0.1.0",
  "scripts": {
    "start": "gulp dev --serve --watch",
    "bundle": "gulp dev",
    "prebuild": "gulp clean",
    "build": "gulp build",
    "prezipbuild": "npm run build",
    "zipbuild": "gulp zip"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "livereactload/babel-transform",
                  "imports": [
                    "react"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "browser-jsonp": "^1.1.5",
    "classnames": "^2.2.5",
    "lodash": "^4.17.2",
    "lodash-es": "^4.17.2",
    "moment": "^2.17.1",
    "react": "^15.3.2",
    "react-datetime": "^2.7.5",
    "react-dom": "^15.3.2",
    "react-lazy-load": "^3.0.10",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.17.5",
    "browserify": "^13.3.0",
    "eslint": "^3.16.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.10.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-gzip": "^1.4.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.8",
    "livereactload": "^3.2.0",
    "react-proxy": "^1.1.8",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream2": "^0.1.1",
    "watchify": "^3.9.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourapp.git"
  }
}
