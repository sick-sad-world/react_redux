{
  "name": "trendolizer_webpack",
  "version": "0.0.1",
  "description": "Trendolizer pro",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourapp.git"
  },
  "author": "Igor Koptelov",
  "license": "MIT",
  "scripts": {
    "test": "",
    "clean": "rimraf ./demo ./build",
    "postclean": "rimraf ./assets/*.js ./assets/*.css ./assets/*.map",
    "prestart": "rimraf ./assets/*.js ./assets/*.css ./assets/*.map",
    "start": "cross-env NODE_ENV=development gulp serve --base ./assets",
    "prebundle": "rimraf ./demo",
    "bundle": "cross-env NODE_ENV=development gulp bundle --base ./assets --dir ./demo",
    "prebuild": "rimraf ./build",
    "build": "cross-env NODE_ENV=production gulp build --base ./assets --dir ./build"
  },
  "browserify": {
    "transform": [
      "babelify",
      "aliasify"
    ]
  },
  "aliasify": {
    "aliases": {
      "src": "./assets/src",
      "common": "./assets/src/_common",
      "functions": "./assets/src/_common/functions"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "lodash"
    ],
    "env": {
      "production": {
        "presets": [
          "react-optimize"
        ]
      },
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "livereactload/babel-transform",
                  "imports": [
                    "react"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "browser-jsonp": "^1.1.5",
    "classnames": "^2.2.5",
    "cross-env": "^5.0.1",
    "lodash-es": "^4.17.2",
    "moment": "^2.17.1",
    "react": "^15.3.2",
    "react-datetime": "^2.7.5",
    "react-dom": "^15.3.2",
    "react-lazy-load": "^3.0.10",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-select": "^1.0.0-rc.2",
    "react-sortable-hoc": "^0.6.3",
    "react-transition-group": "^1.2.0",
    "react-virtualized": "^9.7.5",
    "recharts": "^1.0.0-alpha.0",
    "prop-types": "^15.5.8",
    "redux": "^3.6.0",
    "redux-observable": "^0.14.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4",
    "rxjs": "^5.4.1"
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "babel-core": "^6.24.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.17.5",
    "browserify": "^13.3.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-gzip": "^1.4.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.8",
    "livereactload": "^3.2.0",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.4.2",
    "react-proxy": "^1.1.8",
    "rimraf": "^2.6.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream2": "^0.1.1",
    "watchify": "^3.9.0"
  }
}
