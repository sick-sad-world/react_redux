{
  "name": "trendolizer_ui",
  "version": "0.0.1",
  "description": "",
  "main": "assets/app.js",
  "scripts": {
    "test": "jest --json --outputFile=jest-test-results.json",
    "start": "webpack-dev-server --inline --port=3000",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook-bundle": "build-storybook -c .storybook -o framework-demo",
    "prebundle": "rimraf ./dist",
    "bundle": "webpack --progress",
    "prebuild": "rimraf ./build",
    "build": "webpack -p --progress",
    "clean": "rimraf ./dist ./build ./coverage jest-test-results.json",
    "precommit": "npm test"
  },
  "author": "Ihor Koptielov",
  "license": "MIT",
  "jest": {
    "notify": false,
    "verbose": true,
    "roots": [
      "<rootDir>/assets"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "story.js",
      "jest-test-results.json"
    ],
    "modulePaths": [
      "<rootDir>/assets/src/shared"
    ],
    "moduleNameMapper": {
      "^shared(.*)$": "<rootDir>/assets/src/shared$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/file-mock.js",
      "\\.(css|sass|scss|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-beautiful-dnd": "^7.1.2",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "react-select": "^1.2.1",
    "react-transition-group": "^2.3.1",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@dump247/storybook-state": "^1.2.3",
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-info": "^3.3.14",
    "@storybook/addon-jest": "^3.3.14",
    "@storybook/addon-options": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.0.0",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^4.0.0",
    "react-test-utils": "0.0.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1",
    "yargs": "^11.0.0"
  }
}
